@-webkit-keyframes toTop{
	0% {
		-webkit-transform:translateY(0%);
		transform:translateY(0%);
		opacity: 1;
	}
	100% {
		-webkit-transform:translateY(-100%);
		transform:translateY(-100%);
		opacity: 0;
	}
}
/* 隐藏元素 */
.fadeOut{
	-webkit-animation:fadeOut 0.5s 0s ease both;
	animation:fadeOut 0.5s 0s ease both;
}
@-webkit-keyframes fadeOut{
	0%{
		-webkit-transform: rotateZ(0deg) rotateY(0deg);
		transform: rotateZ(0deg) rotateY(0deg);
		opacity: 1;
	}
	100%{
		-webkit-transform: rotateZ(360deg) rotateY(0deg);
		transform: rotateZ(360deg) rotateY(0deg);
		opacity: 0;
	}
}
@keyframes fadeOut{
	0%{
		-webkit-transform: rotateZ(0deg) rotateY(0deg);
		transform: rotateZ(0deg) rotateY(0deg);
		opacity: 1;
	}
	100%{
		-webkit-transform: rotateZ(360deg) rotateY(0deg);
		transform: rotateZ(360deg) rotateY(0deg);
		opacity: 0;
	}
}
/* 翻转动画 */
.flip{
	-webkit-animation: flip 0.35s 0s ease both;
	animation: flip 0.35s 0s ease both;
}
.flip2{
	-webkit-animation: flip2 0.35s 0s ease both;
	animation: flip2 0.35s 0s ease both;
}
@-webkit-keyframes flip{ /*转到背面*/
	0%{
		-webkit-transform: rotateY(0deg);
		transform: rotateY(0deg);
	}
	100%{
		-webkit-transform: rotateY(180deg);
		transform: rotateY(180deg);
	}
}
@keyframes flip{ /*转到背面*/
	0%{
		-webkit-transform: rotateY(0deg);
		transform: rotateY(0deg);
	}
	100%{
		-webkit-transform: rotateY(180deg);
		transform: rotateY(180deg);
	}
}
@-webkit-keyframes flip2{/*从背面转回来*/
	0%{
		-webkit-transform: rotateY(180deg);
		transform: rotateY(180deg);
	}
	100%{
		-webkit-transform: rotateY(0deg);
		transform: rotateY(0deg);
	}
}
@keyframes flip2{/*从背面转回来*/
	0%{
		-webkit-transform: rotateY(180deg);
		transform: rotateY(180deg);
	}
	100%{
		-webkit-transform: rotateY(0deg);
		transform: rotateY(0deg);
	}
}
/* 晃动 */
.swing{
	-webkit-animation:swing 0.5s 0s ease both;
	animation:swing 0.5s 0s ease both;
}
/*晃动----没用到*/
@-webkit-keyframes swing{
	0%{-webkit-transform:translateX(0%);transform:translateX(0%); }
	15%{-webkit-transform:translateX(-25%) rotateZ(-5deg);transform:translateX(-25%) rotateZ(-5deg); }
	30%{-webkit-transform:translateX(20%) rotateZ(3deg);transform:translateX(20%) rotateZ(3deg); }
	45%{-webkit-transform:translateX(-15%) rotateZ(-3deg);transform:translateX(-15%) rotateZ(-3deg); }
	60%{-webkit-transform:translateX(10%) rotateZ(2deg);transform:translateX(10%) rotateZ(2deg); }
	75%{-webkit-transform:translateX(-5%) rotateZ(-1deg);transform:translateX(-5%) rotateZ(-1deg); }
	100%{-webkit-transform:translateX(0%);transform:translateX(0%); }
}
@keyframes swing{
	0%{-webkit-transform:translateX(0%);transform:translateX(0%); }
	15%{-webkit-transform:translateX(-25%) rotateZ(-5deg);transform:translateX(-25%) rotateZ(-5deg); }
	30%{-webkit-transform:translateX(20%) rotateZ(3deg);transform:translateX(20%) rotateZ(3deg); }
	45%{-webkit-transform:translateX(-15%) rotateZ(-3deg);transform:translateX(-15%) rotateZ(-3deg); }
	60%{-webkit-transform:translateX(10%) rotateZ(2deg);transform:translateX(10%) rotateZ(2deg); }
	75%{-webkit-transform:translateX(-5%) rotateZ(-1deg);transform:translateX(-5%) rotateZ(-1deg); }
	100%{-webkit-transform:translateX(0%);transform:translateX(0%); }
}
/* 旋转淡入 */
.rotateIn{
	-webkit-animation:rotateIn 1s 0s ease both;
	animation:rotateIn 1s 0s ease both;
}
@-webkit-keyframes rotateIn{
	0%{
		-webkit-transform-origin:center center;
		transform-origin:center center;
		-webkit-transform:rotate(-200deg);
		transform:rotate(-200deg);
		opacity:0;
	}
	100%{
		-webkit-transform-origin:center center;
		transform-origin:center center;
		-webkit-transform:rotate(0);
		transform:rotate(0);
		opacity:1;
	}
}
@keyframes rotateIn{
	0%{
		-webkit-transform-origin:center center;
		transform-origin:center center;
		-webkit-transform:rotate(-200deg);
		transform:rotate(-200deg);
		opacity:0;
	}
	100%{
		-webkit-transform-origin:center center;
		transform-origin:center center;
		-webkit-transform:rotate(0);
		transform:rotate(0);
		opacity:1;
	}
}
.zoomBig {
	-webkit-animation: zoomBig 1s 0s ease both;
	animation: zoomBig 1s 0s ease both;
}
.zoomBig1 {
	-webkit-animation: zoomBig 1s .2s ease both;
	animation: zoomBig 1s .2s ease both;
}
.zoomBig2 {
	-webkit-animation: zoomBig 1s .4s ease both;
	animation: zoomBig 1s .4s ease both;
}
.zoomBig3 {
	-webkit-animation: zoomBig 1s .6s ease both;
	animation: zoomBig 1s .6s ease both;
}
/*气泡放大*/
@-webkit-keyframes zoomBig{
	0% {
		-webkit-transform: scale(0.1);
		transform: scale(0.1);
		opacity: 1;
	}
	/*50%{
		-webkit-transform: scale(1);
		transform: scale(1);
		opacity: .8;
	}*/
	100%{
		-webkit-transform: scale(1.8) translateY(-10px);
		transform: scale(1.8) translateY(-10px);
		opacity: 0;
	}
}
@keyframes zoomBig{
	0% {
		-webkit-transform: scale(0.1);
		transform: scale(0.1);
		opacity: 1;
	}
	/*50%{
		-webkit-transform: scale(1);
		transform: scale(1);
		opacity: .8;
	}*/
	100%{
		-webkit-transform: scale(1.8) translateY(-10px);
		transform: scale(1.8) translateY(-10px);
		opacity: 0;
	}
}
.zoomUp {
	-webkit-animation: zoomUp 1s 0s ease both;
	animation: zoomUp 1s 0s ease both;
}
/*分数*/
@-webkit-keyframes zoomUp{
	0% {
		-webkit-transform: scale(0.5);
		transform: scale(0.5);
		opacity: 1;
	}
	100%{
		-webkit-transform: scale(1) translateY(-20px);
		transform: scale(1) translateY(-20px);
		opacity: 0;
	}
}
@keyframes zoomUp{
	0% {
		-webkit-transform: scale(0.5);
		transform: scale(0.5);
		opacity: 1;
	}
	100%{
		-webkit-transform: scale(1) translateY(-20px);
		transform: scale(1) translateY(-20px);
		opacity: 0;
	}
}
/*心碎裂*/
.heartLeft {
	-webkit-animation: heartLeft 0.6s 0s ease both;
	animation: heartLeft 0.6s 0s ease both;
}
.heartRight {
	-webkit-animation: heartRight 1s 0s ease both;
	animation: heartRight 1s 0s ease both;
}
@-webkit-keyframes heartLeft{
	0% {
		-webkit-transform-origin: right bottom;
		transform-origin: right bottom;
		-webkit-transform:rotate(10deg);
		transform:rotate(10deg);
	}
	100%{
		-webkit-transform-origin: right bottom;
		transform-origin: right bottom;
		-webkit-transform:rotate(-11deg);
		transform:rotate(-11deg);
	}
}
@keyframes heartLeft{
	0% {
		-webkit-transform-origin: right bottom;
		transform-origin: right bottom;
		-webkit-transform:rotate(10deg);
		transform:rotate(10deg);
	}
	100%{
		-webkit-transform-origin: right bottom;
		transform-origin: right bottom;
		-webkit-transform:rotate(-11deg);
		transform:rotate(-11deg);
	}
}
@-webkit-keyframes heartRight{
	0% {
		-webkit-transform-origin: left bottom;
		transform-origin: left bottom;
		-webkit-transform:rotate(-10deg);
		transform:rotate(-10deg);
	}
	100%{
		-webkit-transform-origin: left bottom;
		transform-origin: left bottom;
		-webkit-transform:rotate(10deg);
		transform:rotate(10deg);
	}
}
@keyframes heartRight{
	0% {
		-webkit-transform-origin: left bottom;
		transform-origin: left bottom;
		-webkit-transform:rotate(-10deg);
		transform:rotate(-10deg);
	}
	100%{
		-webkit-transform-origin: left bottom;
		transform-origin: left bottom;
		-webkit-transform:rotate(10deg);
		transform:rotate(10deg);
	}
}
