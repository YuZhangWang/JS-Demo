#cards_box{
	overflow: hidden;
	width: 100%;
	padding: 0.5% 0 0 0.5%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.card{
	position: relative;
	float: left;
	/*width: 25%;
	margin: 4%;
 	padding-bottom: 24.5%; */
 	width: 216.6rpx;
 	height: 216.6rpx;
 	margin: 13.12rpx;
	text-align: center;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	overflow: hidden;
}
.card view{
	-webkit-transform: rotateY(0deg);
	transform: rotateY(0deg);
	position: absolute;
	top: 0.0rpx;
	left: 0.0rpx;
	width: 100%;
	height: 100%;	
	-webkit-border-radius: 5px;
	border-radius: 5px;
} 
.card view:nth-child(1){
	z-index: 1;
	-webkit-backface-visibility:hidden;	/* Chrome 和 Safari */
	-moz-backface-visibility:hidden; 	/* Firefox */
	-ms-backface-visibility:hidden; 	/* Internet Explorer */
	backface-visibility: hidden; /* 背面不可见 */
}
.card view image{
	width: 100%;
	height: 100%;
	/*object-fit: cover;*/ /*对替换元素而言 对img本身无用*/
}
.card view:nth-child(2) image{
	-webkit-transform: rotateY(180deg);
	transform: rotateY(180deg); /*将背面元素转到背面正向Z轴负向*/
}
