/* pages/selfAddress/selfAddress.wxss */
.container{
  width: 100%;
  height: 100%;
}
.item{
  margin: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-bottom: 4px solid #f5f5f5;
  padding: 10rpx 0;
}
.item-left{
  width: 85%;
}
.item-right{
  width: 15%;
  display: flex;
  align-items: center;
  justify-content: center;

}
.addressName text:nth-child(1){
  font-size: 30rpx;
}
.addressName text:nth-child(2){
  font-size: 26rpx;
  color: #959595;
  margin-left: 25rpx;
}
.address{
  font-size: 26rpx;
  color: #959595;
  margin-top: 15rpx;
  overflow: hidden;
  
}
.icon-arr {
  display:block;
  width:26rpx;
  height:26rpx;

}
